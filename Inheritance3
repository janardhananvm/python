import random

class Animal:
    pass
    info = "FROM ANIMAL: a living organism that feeds on organic matter"

    def __init__(self,name):
        print("An animal is created")
        self.name = name

class Dog(Animal):
    information = "This is Dig information sentence"

    info = "FROM DOG: a living organism that feeds on organic matter"

    def __init__(self, name):
        print("in self of Dog")
        super().__init__(name)
        print("A dog is created")
        self.lucky_number = random.randint(1,20)
        self.name = name
    
    def bark(self):
        print(f"Woof Woof!! my name is {self.name} & my number is {self.lucky_number}")

class Bulldog(Dog):
    def __init__(self,name):
        super().__init__(name)
        print("A bulldog is created")

dog1 = Dog("Fido")
dog1.bark()
print(dog1.info)

dog2 = Bulldog("rot")
